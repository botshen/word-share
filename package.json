{
  "name": "word-share",
  "displayName": "Word share",
  "version": "0.0.1",
  "description": "A basic Plasmo extension.",
  "author": "Shen Xin. <botshen@zgbj@gmail.com>",
  "scripts": {
    "dev": "plasmo dev",
    "build": "plasmo build",
    "package": "plasmo package"
  },
  "dependencies": {
    "@plasmohq/messaging": "^0.6.2",
    "@plasmohq/storage": "^1.10.0",
    "downloadjs": "^1.4.7",
    "html-to-image": "^1.11.11",
    "html2canvas": "^1.4.1",
    "plasmo": "0.86.3",
    "re-resizable": "^6.9.16",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-quill": "^2.0.0"
  },
  "devDependencies": {
    "@ianvs/prettier-plugin-sort-imports": "4.1.1",
    "@types/chrome": "0.0.258",
    "@types/downloadjs": "^1.4.6",
    "@types/node": "20.11.5",
    "@types/react": "18.2.48",
    "@types/react-dom": "18.2.18",
    "prettier": "3.2.4",
    "typescript": "5.3.3"
  },
  "manifest": {
    "host_permissions": [
      "https://*/*"
    ],
    "permissions": [
      "scripting",
      "storage",
      "activeTab",
      "tabs"
    ],
    "web_accessible_resources": [
      {
        "matches": [
          "<all_urls>"
        ],
        "resources": [
          "tabs/App.html"
        ]
      }
    ]
  }
}
